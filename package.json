{
  "name": "desktop-management-dashboard",
  "version": "1.0.0",
  "description": "Simple electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:osx": "electron-packager . DevDashboard --platform=darwin --arch=x64 --icon=./icon-snews.png --out=release-builds --overwrite",
    "build:raspbian": "electron-packager . DevDashboard --platform=linux --arch=armv7l --icon=./icon-snews.png --out=release-builds --overwrite"
  },
  "repository": "https://github.com/baufaker/desktop-management-dashboard",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-packager": "*"
  }
}
